    "ts-loader": "^9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.9.1",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
